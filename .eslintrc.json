{
    "env": {
        "node": true,
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "func-names": 0,
        "array-callback-return": 0,
        "import/no-dynamic-require": 0,
        "global-require": 0,
        "no-use-before-define": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0
    }
}
